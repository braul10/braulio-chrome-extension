{
  "manifest_version": 3,
  "name": "Braulio's extension",
  "version": "1.0",
  "description": "Distintas funcionalidades para mejorar la productividad",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "open_chatgpt": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Open a specific URL"
    },
    "open_maps_in_incognito": {
      "suggested_key": {
        "default": "Alt+M"
      },
      "description": "Open Google Maps in Incognito"
    },
    "open_github": {
      "suggested_key": {
        "default": "Alt+G"
      },
      "description": "Open Github"
    },
    "openIncognitoAndClose": {
      "suggested_key": {
        "default": "Alt+I"
      },
      "description": "Open X"
    }
  }
}